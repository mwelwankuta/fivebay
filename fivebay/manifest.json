{
  "manifest_version": 3,
  "name": "Fivebay",
  "description": "Filters eBay search results to only show auctions ending in five minutes.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "64": "icon.png"
    }
  },
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "https://www.ebay.com/*",
    "http://localhost:8080/*"
  ]
}
